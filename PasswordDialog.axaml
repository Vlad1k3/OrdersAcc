<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OrdersAcc.PasswordDialog"
        Title="Admin Password" Width="300" Height="150">
    <StackPanel Margin="10">
        <TextBlock Text="Введите пароль администратора:" Margin="0,0,0,10"/>
        <TextBox Name="passwordBox" PasswordChar="*" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,10,0,0">
            <Button Name="okButton" Content="OK" Margin="0,0,10,0" Click="OkButton_Click"/>
            <Button Name="cancelButton" Content="Cancel" Click="CancelButton_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
