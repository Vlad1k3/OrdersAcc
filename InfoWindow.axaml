<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OrdersAcc.InfoWindow"
        Title="InfoWindow"
        Width="700"
        Height="500"
        CanResize="False">
    <Grid>
        <StackPanel Margin="20" x:Name="MainStackPanel">
            <TextBox x:Name="EqTypeTextBox" FontSize="16" IsReadOnly="True" />
            <TextBox x:Name="EqModelTextBox" FontSize="16" IsReadOnly="True" />
            <TextBox x:Name="StatusTextBox" FontSize="16" IsReadOnly="True" />
            <TextBox x:Name="UserNameTextBox" FontSize="16" IsReadOnly="True" />
            <TextBox x:Name="StartDateTextBox" FontSize="16" IsReadOnly="True" />
            <TextBox x:Name="CompletionDateTextBox" FontSize="16" IsReadOnly="True" />
            <TextBox x:Name="ProblemDescTextBox" FontSize="16" IsReadOnly="True" />
            <Button x:Name="DeleteOrderButton" Click="DeleteOrderButton_Click" Content="Удалить запись" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
