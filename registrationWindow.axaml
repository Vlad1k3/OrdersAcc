<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        d:DesignWidth="400"
        d:DesignHeight="200"
        x:Class="OrdersAcc.registrationWindow"
        Title="Create Order">

    <Grid Margin="20">
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
            <TextBlock Text="ФИО:"/>
            <TextBox Name="txtFIO" Width="200"/>
            <TextBlock Text="Номер телефона:"/>
            <TextBox Name="txtPhoneNumber" Width="200"/>
            <TextBlock Text="Логнин:"/>
            <TextBox Name="txtLogin" Width="200"/>
            <TextBlock Text="Пароль:"/>
            <TextBox Name="txtPassword" Width="200"/>
            <TextBlock Text="Должность:"/>
            <TextBox Name="txtJobtitle" Width="200"/>
            <Button Content="Создать аккаунт" HorizontalAlignment="Center" Click="CreateProfilerButton_Click" Width="150"/>
        </StackPanel>
    </Grid>
</Window>
